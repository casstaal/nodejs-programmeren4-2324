# 
PORT=3000
JWT_SECRET=verysecretkey
LOGLEVEL=trace 
DB_HOST=localhost
DB_PORT=3306
DB_USER=share-a-meal-user
DB_PASSWORD=secret
DB_DATABASE=share-a-meal